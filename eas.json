{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_NO_CACHE": "1"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_NO_CACHE": "1",
        "EXPO_IMAGE_UTILS_NO_CACHE": "1",
        "EXPO_CACHE_DIR": "/tmp/expo-cache"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "env": {
        "EXPO_NO_CACHE": "1",
        "EXPO_IMAGE_UTILS_NO_CACHE": "1",
        "EXPO_CACHE_DIR": "/tmp/expo-cache"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}


